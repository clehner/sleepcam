[
	{
		"from": "images/pics/large/:pic",
		"to": "../../:pic/large.jpg"
	},
	{
		"from": "images/pics/small/:pic",
		"to": "../../:pic/small.jpg"
	},
	{
		"from": "images/pics/large/",
		"to": "images/transparent.gif"
	},
	{
		"from": "images/pics/small/",
		"to": "images/transparent.gif"
	},

	{
		"from": "images/profile/:profile-name",
		"to": "_show/profile-pic/:profile-name"
	},

	{
		"from": "",
		"to": "_list/index/pics",
		"query": {
			"descending": "true",
			"limit": "20",
			"reduce": "false"
		}
	},

	{
		"from": "profile/:user",
		"to": "_list/profile-stream/pics-by-user",
		"query": {
			"include_docs": "true",
			"descending": "true",
			"startkey": [":user", {}],
			"endkey": [":user"],
			"limit": "100"
		}
	},

	{
		"from": "pic/:id",
		"to": "_show/pic/:id",
		"method": "GET"
	},
	{
		"from": "pic/:id",
		"to": "_update/pic/:id",
		"method": "POST"
	},

	{
		"from": "login",
		"to": "_show/login/"
	},
	{
		"from": "signup",
		"to": "signup.html"
	},

	{
		"from": "_changes",
		"to": "../../_changes"
	},
	{
		"from": "favicon.ico",
		"to": "images/favicon.ico"
	},
	{
		"from": "downloads/Sleepcam.dmg",
		"to": "downloads/Sleepcam-1.0.2.dmg"
	},
	{
		"from": "*",
		"to": "*"
	}
]
