[
	{
		"from": "images/pics/large/:pic",
		"to": "../../:pic/large.jpg"
	},
	{
		"from": "images/pics/small/:pic",
		"to": "../../:pic/small.jpg"
	},
	{
		"from": "images/profile/:profile-name",
		"to": "_show/profile-pic/:profile-name"
	},

	{
		"from": "profile/:user",
		"to": "_list/profile-stream/pics-by-user",
		"query": {
			"include_docs": "true",
			"descending": "true",
			"startkey": [":user", {}],
			"endkey": [":user"],
			"limit": "100"
		}
	},

	{
		"from": "pic/:id",
		"to": "_show/pic/:id",
		"method": "GET"
	},
	{
		"from": "pic/:id",
		"to": "_update/pic/:id",
		"method": "POST"
	},

	{
		"from": "",
		"to": "index.html"
	},
	{
		"from": "favicon.ico",
		"to": "images/favicon.ico"
	},
	{
		"from": "*",
		"to": "*"
	}
]
